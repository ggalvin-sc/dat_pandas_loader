          "right": "EndAttach_Right"
        }
      },
      {
        "source_field": "family range",
        "separator": "-",
        "target_fields": {
          "left": "BegAttach",
          "right": "EndAttach"
        }
      }
    ],
    "merges": [
      {
        "source_fields": {
          "date": "DateSent",
          "time": "TimeSent"
        },
        "target_field": "SentDateTime",
        "format": {
          "date": "%Y-%m-%d",
          "time": "%H:%M:%S",
          "output": "%Y-%m-%dT%H:%M:%S"
        }
      },
      {
        "source_fields": {
          "date": "DateReceived",
          "time": "TimeReceived"
        },
        "target_field": "ReceivedDateTime",
        "format": {
          "date": "%Y-%m-%d",
          "time": "%H:%M:%S",
          "output": "%Y-%m-%dT%H:%M:%S"
        }
      }
    ]
